{
	"name": "admin",
	"version": "0.1.2",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "dotenv -e .dev.env -- npm run dev:with-env",
		"dev:app": "vite --port 3101",
		"dev:with-env": "npm run docker:up && npm run db:populate:dev && npm run s3:populate:dev && npm run dev:app",
		"dev:full": "npm run docker:up && npm run dev",
		"build": "vite build",
		"start": "node dist/server/server.js",
		"lint": "biome lint",
		"format": "biome format --write",
		"check": "biome check --write",
		"test": "dotenv -e .test.env -- npm run test:all",
		"test:all": "npm run test:unit && npm run test:e2e",
		"test:unit": "vitest run",
		"test:e2e": "playwright test",
		"coverage:all": "npm run coverage:unit && npm run coverage:e2e && npm run coverage:merge",
		"coverage:unit": "vitest run --coverage",
		"coverage:e2e": "playwright test --reporter=html",
		"coverage:merge": "node --import tsx ./tests/util/merge-coverage.mts",
		"docker:up": "docker compose -f ../../devops/docker-compose.yml up -d",
		"docker:down": "docker compose -f ../../devops/docker-compose.yml down",
		"db:populate:dev": "cd ../../data && cargo make mysql-populate-dev",
		"db:populate:test": "cd ../../data && cargo make mysql-populate-test",
		"s3:populate:dev": "cd ../../data && cargo make s3-populate-dev",
		"s3:populate:test": "cd ../../data && cargo make s3-populate-test"
	},
	"engines": {
		"node": ">=24.11.1 <24.12.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.700.0",
		"@aws-sdk/s3-request-presigner": "^3.700.0",
		"@tanstack/react-query": "^5.60.0",
		"@tanstack/react-router": "^1.114.0",
		"@tanstack/react-router-devtools": "^1.114.0",
		"@tanstack/react-start": "^1.114.0",
		"@tiptap/extension-image": "^2.10.0",
		"@tiptap/extension-link": "^2.10.0",
		"@tiptap/extension-placeholder": "^2.10.0",
		"@tiptap/pm": "^2.10.0",
		"@tiptap/react": "^2.10.0",
		"@tiptap/starter-kit": "^2.10.0",
		"flowbite": "^4.0.1",
		"flowbite-react": "^0.12.16",
		"jose": "^6.1.3",
		"mysql2": "^3.16.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"uuid": "^11.0.0",
		"vinxi": "^0.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@playwright/test": "^1.57.0",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.9",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-react": "^4.5.2",
		"@vitest/coverage-v8": "^3.2.0",
		"autoprefixer": "^10.4.23",
		"dotenv-cli": "^7.4.4",
		"jsdom": "^27.4.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.0",
		"typescript": "^5.9.3",
		"vite": "^7.0.0",
		"vite-tsconfig-paths": "^6.0.0",
		"vitest": "^3.2.0"
	}
}
